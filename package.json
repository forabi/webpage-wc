{
  "name": "webpage-wc",
  "version": "1.0.3",
  "description": "Prints number of words in one or more webpages using the Readablity API",
  "main": "index.js",
  "bin": {
    "webpage-wc": "./index.js"
  },
  "scripts": {
    "test": "npm run lint && mocha src/**/*.spec.js",
    "build": "babel src/index.js -o ./index.js",
    "lint": "eslint src",
    "coverage": "rm -rf coverage && babel-node ./node_modules/.bin/babel-istanbul cover _mocha src/**/*.spec.js",
    "prepublish": "npm run test && npm run build"
  },
  "keywords": [],
