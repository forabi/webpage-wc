  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/forabi/webpage-wc.git"
  },
  "bugs": {
    "url": "https://github.com/forabi/webpage-wc/issues"
  },
  "dependencies": {
    "appdirs": "^1.0.0",
    "cheerio": "^0.20.0",
    "got": "^6.1.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "pluralize": "^1.2.1",
    "easy-table": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-istanbul": "^0.6.0",
    "babel-plugin-transform-async-functions": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "coveralls": "^2.11.6",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "mocha": "^2.4.5"
  }
}
